{
  "blocked_patterns": [
    ".*\\.env\\.production.*",
    ".*\\.env\\.prod.*",
    ".*\\.pem$",
    ".*\\.key$",
    ".*\\.cert$",
    ".*\\.p12$",
    ".*\\.pfx$",
    ".*production\\.config.*",
    ".*prod\\.config.*",
    ".*/\\.ssh/.*",
    ".*/\\.aws/.*",
    ".*/\\.azure/.*",
    ".*/\\.gcp/.*",
    ".*/\\.git/config$",
    ".*/\\.git/hooks/.*",
    ".*secret.*",
    ".*password.*",
    ".*credential.*",
    ".*token.*",
    ".*\\.log$",
    ".*backup.*\\.sql$",
    ".*dump.*\\.sql$",
    ".*\\.bak$",
    ".env"
  ],
  "blocked_directories": [
    "/etc",
    "/root",
    "/var/log",
    "/var/secrets",
    "/opt/secrets",
    "C:\\Windows\\System32",
    "C:\\Program Files",
    "C:\\ProgramData",
    "/usr/local/secrets",
    "/home/.ssh",
    "~/.ssh",
    "~/.aws",
    "~/.azure",
    "~/.gcp"
    
  ],
  "allowed_directories": [
    "./.propel",
    "./.claude",
    "./src",
    "./lib",
    "./components",
    "./pages",
    "./public",
    "./assets",
    "./docs",
    "./test",
    "./tests",
    "./spec",
    "./examples"
  ],
  "_comment": "Customize these patterns based on your project's security requirements. The script will allow access to files in allowed_directories and deny access to files matching blocked_patterns or in blocked_directories."
}